<?xml version="1.0" encoding="utf-8"?>
<!-- Changes to this file should also be reflected in ../WEB-INF-dev/appengine-web.xml -->
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>replicated-qa</application>
  <precompilation-enabled>false</precompilation-enabled>
  <version>10</version>
  <threadsafe>true</threadsafe>
  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    <property name="com.google.testing.testify.risk.frontend.url" value="https://replicated-qa.appspot.com" />
    <property name="com.google.testing.testify.risk.frontend.localdomain" value="@replicated.com" />
    <property name="com.google.testing.testify.risk.frontend.whitelisting" value="false" />
  </system-properties>
</appengine-web-app>
